    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "git-rev-sync": "^1.12.0",
    "raw-loader": "^1.0.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.27.0"
  }
