{
  "name": "mathhub-react-frontend",
  "version": "15.1.0",
  "description": "A rewrite of the MathHub Frontend in React",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "mklegal": "yarn --ignore-platform licenses generate-disclaimer > src/assets/generated/notices.txt",
    "dist": "yarn mklegal && yarn build && yarn export",
    "devmmt": "cross-env MMT_URL=http://localhost:8080/:mathhub/ next dev",
    "devmock": "cross-env MMT_URL= next dev"
  },
  "dependencies": {
    "@types/xmldom": "^0.1.29",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
