    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "next-server": "^0.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-waypoint": "^8.1.0",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.84.0",
    "ts-debounce": "^1.0.0",
    "xmldom": "^0.1.27"
  },
  "browserslist": "last 2 versions, not dead, not IE > 0, Firefox ESR",
  "license": "AGPL-3.0",
